{
  "name": "@discordts/client",
  "version": "0.0.0",
  "description": "Modular discord client written in typescript!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run build -- -w",
    "lint": "tslint -t stylish --project tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "clean": "rimraf dist",
    "precommit": "npm run lint && npm run test",
    "release": "npm run precommit && standard-version && git push --follow-tags origin master && npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "author": "Nurdism <nurdsim.io@gmail.com>",
  "homepage": "https://github.com/discordts/client#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discordts/client.git"
  },
  "bugs": {
    "url": "https://github.com/discordts/client/issues"
  },
  "keywords": [
    "discord",
    "api",
    "bot",
    "client",
    "node",
    "discordapp"
  ]
}
